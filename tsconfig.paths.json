    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@aliases/*": [
        "./src/aliases/*"
      ],
      "@enums/*": [
        "./src/enums/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@contents/*": [
        "./src/contents/*"
      ],
      "@hocs/*": [
        "./src/hocs/*"
      ],
      "@interfaces/*": [
        "./src/interfaces/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@libs/*": [
        "./src/libs/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@templates/*": [
        "./src/templates/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    }
  }
}